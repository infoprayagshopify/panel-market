                    <div class="loginSide">
                        <form method="post" action="">

          <div class="form-group">
            <label for="username" class="control-label">{{ lang['resetpassword.user'] }}</label>
            <input type="text" class="form-control" name="user" value="{{ data['user'] }}" placeholder="{{ lang['resetpassword.user.placeholder'] }}">
          </div>

          <div class="form-group">
            <label for="username" class="control-label">{{ lang['resetpassword.type'] }}</label>
            <select class="form-control" name="type">
             {% for reset in resetType %}
              <option value="{{ reset['type'] }}">{{ reset['name'] }}</option>
             {% endfor %}
            </select>
          </div>


          {% if captcha %}
            <div class="form-group">
              <div class="g-recaptcha" data-sitekey="{{ captchaKey }}"></div>
            </div>
          {% endif %}

            <button type="submit" class="btn btn-block btn-big-primary">{{ lang['resetpassword.button'] }}</button>
        </form>

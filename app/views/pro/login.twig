						   <div class="col-md-12">

      {% if errorText %}
        <div class="alert alert-dismissible alert-danger">
          <button type="button" class="close" data-dismiss="alert">&times;</button>
          {{ errorText }}
        </div>
      {% endif %}
      {% if successText %}
        <div class="alert alert-dismissible alert-success">
          <button type="button" class="close" data-dismiss="alert">&times;</button>
          {{ successText }}
        </div>
      {% endif %}

</div><br>
                     
                            <form class="component_form_group component_checkbox_remember_me" method="post" action="">
                                <div class="">
                                    
                                                                        <div class="form-group">
                                        <label>{{ lang['signin.username'] }}</label>
                                        <input type="text" name="username" class="form-control">
                                    </div>
                                    <div class="form-group">
                                        <label>{{ lang['signin.password'] }}</label>
                                        <div class="position-relative">
                                            <input type="password" name="password" class="form-control">{% if resetPage %}
                                                                                            <div class="sign-in__forgot h-100 d-flex align-items-center">
                                                    <a href="resetpassword">{{ lang['signin.password.lost'] }}</a>
                                                </div>{% endif %}
                                                                                    </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="form-group__checkbox">
                                            <label class="form-group__checkbox-label">
                                                <input type="checkbox" name="remember" id="block_2_remember_me" value="1">
                                                <span class="checkmark"></span>
                                            </label>
                                            <label class="form-group__label-title" for="block_2_remember_me">{{ lang['signin.remember'] }}</label>
                                        </div>
                                    </div>
                                    {% if captcha %}
            <div class="form-group">
              <div class="g-recaptcha" data-sitekey="{{ captchaKey }}"></div>
            </div>
          {% endif %}
                                    <div class="form-group">
                                        <div class="component_button_submit">
                                            <div class="">
                                                <button class="btn btn-block btn-big-primary" type="submit">
                                                    {{ lang['signin.button'] }}
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                
                                                                    <div class="text-center d-flex justify-content-center">
                                        <div>Do not have an account?</div>
